    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-solid": "0.14.3"
  },
  "peerDependencies": {
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/eslint": "9.6.1",
    "@types/node": "22.8.1",
    "@typescript-eslint/parser": "8.11.0"
